{
  "devDependencies": {
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-sw-precache": "^1.1.1",
    "sass": "1.20.1",
    "typescript": "^4.0.3",
    "xstate": "^4.13.0"
  },
  "dependencies": {},
